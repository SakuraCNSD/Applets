.user-info{
  height: 150rpx;
  border-bottom: 1px solid #b4b5b6;
  display: flex;
  align-items: center;
}
.user-info image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow: hidden;
  margin: 0 20rpx;
}
.user-list{
  margin-top: 40rpx;
}
.user-list-item{
  height: 50rpx;
  border-bottom: 1px dashed #b4b5b6;
  padding: 10rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}